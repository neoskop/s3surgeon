{
  "name": "@neoskop/s3surgeon",
  "version": "2.0.0",
  "description": "Sync your files to AWS S3 with surgical precision.",
  "main": "lib/index.js",
  "type": "module",
  "keywords": [
    "AWS",
    "S3"
  ],
  "bin": {
    "s3surgeon": "lib/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'node --loader ts-node/esm' src/index.ts",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest --runInBand",
    "build": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neoskop/s3surgeon.git"
  },
  "author": "Arne Diekmann <diekmann@neoskop.de>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/neoskop/s3surgeon/issues"
  },
  "homepage": "https://github.com/neoskop/s3surgeon#readme",
  "dependencies": {
    "aws-sdk": "2.1146.0",
    "chalk": "5.1.2",
    "commander": "9.3.0",
    "mime-types": "2.1.35",
    "p-limit": "4.0.0"
  },
  "devDependencies": {
    "@types/jest": "29.2.3",
    "@types/mime-types": "2.1.1",
    "@types/node": "17.0.38",
    "jest": "29.3.1",
    "nodemon": "2.0.17",
    "ts-jest": "29.0.3",
    "ts-node": "10.8.0",
    "typescript": "4.7.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "roots": [
      "./src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "moduleNameMapper": {
      "#(.*)": "<rootDir>/node_modules/$1",
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  }
}
